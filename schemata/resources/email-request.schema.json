{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "type": "object",
  "properties": {
    "@id": {
      "type": "string",
      "format": "uri",
      "description": "URL of message resource"
    },
    "template": {
      "$ref": "../model/template-identifier.schema.json"
    },
    "parameters": {
      "$ref": "../model/template-parameters.schema.json"
    },
    "requestedAt": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": ["@id", "template", "parameters", "requestedAt"]
}
